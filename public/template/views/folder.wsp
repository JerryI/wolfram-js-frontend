            box-shadow: rgba(15, 15, 15, 0.05) 0px 0px 0px 1px, rgba(15, 15, 15, 0.1) 0px 5px 10px, rgba(15, 15, 15, 0.2) 0px 15px 40px; 
            border-radius: 8px; 
            background-color: white; 
            margin-bottom: 0px; 
            color: inherit;
 
    } 

    .thumbnails-item:hover {
