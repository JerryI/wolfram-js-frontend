<?wsp If[!MissingQ[box], ?>
<div class="styles-notice-box    " id="notification-box">
    <span role="button" tabindex="0" class="styles-dismiss-cross" id="notification-box-button">
        <img src="data:image/svg+xml;base64," alt="close icon">
    </span>
    <h4><?wsp box["title"] ?></h4>
    <p><?wsp box["body"] ?></p>
</div>
<script>
    document.getElementById("notification-box").addEventListener("click", (e)=>{
        document.getElementById("notification-box").remove();
    });
</script>
<?wsp , ""] ?>