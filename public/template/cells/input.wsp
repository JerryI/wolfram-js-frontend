<section class="cell-wrapper frontend-node"  style="opacity: 1;">
    <?wsp LoadPage[FileNameJoin[{JerryI`WolframJSFrontend`public, "template/cells/tools.wsp"}], {hideButtonClass = If[CellObj[Global`id]["props"]["hidden"], "icon-hidden", ""]}] ?>
    <div class="content-wrapper" id="<?wsp Global`id?>">
        <?wsp With[{prop = If[CellObj[Global`id]["props"]["hidden"], " cell-hidden", " " ]}, ?>
            <p class="cell-paragraph frontend-paragraph <?wsp prop ?>" draggable="false" id="<?wsp Global`id?>---input"></p>
        <?wsp ] ?>
    </div>
</section>