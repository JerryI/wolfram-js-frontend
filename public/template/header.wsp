<html><head lang="en">
        <meta charset="utf-8">
        <link rel="stylesheet" href="/dist/merged.css">

        <!-- any inline js including core library (global) -->
        <?wsp Table[ ?> 
        <?wsp Table[ With[{url = FileNameJoin[{Packages[i, "path"], j}]}, ?>
            <script src="/<?wsp url ?>"></script>
        <?wsp ], {j, {Packages[i, "wljs-meta", "js"]}//Flatten}]?>
        <?wsp , {i, Select[Packages//Keys, (Packages[#, "enabled"] && KeyExistsQ[Packages[#, "wljs-meta"], "js"])&]}] ?>
        
        <!-- frontend library (module) -->
        <script type="module" src="/dist/merged.js"></script>
        
        <!-- user's plugins (module) -->
        <?wsp Table[ ?> 
        <?wsp Table[ With[{url = FileNameJoin[{Packages[i, "path"], j}]}, ?>
            <script type="module" src="/<?wsp url ?>"></script>
        <?wsp ], {j, {Packages[i, "wljs-meta", "jsmodule"]}//Flatten}]?>
        <?wsp , {i, Select[Packages//Keys, (Packages[#, "enabled"] && KeyExistsQ[Packages[#, "wljs-meta"], "jsmodule"])&]}] ?>
        
        <!-- connection to the server (global) -->
        <script src="/dist/sockets.js"></script>
    </head>
